{
  "name": "hub",
  "version": "1.0.0",
  "description": "Web hub based on Wikidata",
  "main": "index.js",
  "scripts": {
    "start": "export FORCE_COLOR=true; node index.js",
    "watch": "supervisor -w index.js,lib,config index.js",
    "postinstall": "./scripts/postinstall",
    "lint": "standard",
    "test": "mocha",
    "build": "./scripts/build",
    "build-readme": "./scripts/build_readme && doctoc README.md",
    "build-home": "./scripts/build_home",
    "update-properties": "./scripts/update_properties"
  },
  "keywords": [
    "wikidata",
    "wikipedia",
    "redirections",
    "images"
  ],
  "author": "maxlath",
  "license": "AGPL-3.0",
  "dependencies": {
    "bluebird": "^3.5.1",
    "bluereq": "^2.1.7",
    "chalk": "^2.3.0",
    "config": "^1.28.1",
    "execa": "^0.9.0",
    "express": "^4.16.2",
    "github-markdown-css": "^2.9.0",
    "inv-loggers": "^3.3.1",
    "lodash.keyby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.values": "^4.3.0",
    "serve-favicon": "^2.4.5",
    "wikidata-cli": "^7.5.1",
    "wikidata-sdk": "^7.2.0"
  },
  "devDependencies": {
    "doctoc": "^1.3.0",
    "git-hooks": "^1.1.9",
    "marked": "^0.7.0",
    "mocha": "^4.0.1",
    "should": "^13.1.3",
    "standard": "^10.0.3",
    "supervisor": "^0.12.0"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "it",
      "describe"
    ]
  },
  "engines": {
    "node": "^8.2.1",
    "npm": "^5.4.1"
  }
}
